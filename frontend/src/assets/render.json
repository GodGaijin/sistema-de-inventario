{
  "routes": [
    {
      "src": "/reset-password",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
} 